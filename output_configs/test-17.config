snmp-server location asdf
snmp-server contact NetworkOps
snmp-server host 192.0.2.1 informs version 2c public
snmp-server enable traps
!

enable secret asdf
!

username admin secret asdf
!

ntp server 0.eu.pool.ntp.org
ntp server 1.eu.pool.ntp.org
!

logging host 10.x.x.x
logging trap informational
!


router ospf 1
 network 10.x.x.x 0.0.0.255 area 0
!

spanning-tree mode rapid-pvst
spanning-tree priority 4096
spanning-tree vlan 1-4094 priority 32768
!

ip access-list extended Management_access
 permit ip any 192.0.2.10
 permit ip any 192.0.2.11
 deny ip any any
!
