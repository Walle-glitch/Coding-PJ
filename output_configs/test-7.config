hostname test-7
!
enable secret asdf
username admin password asdf
!
interface Gi1/0/1
 description Access VLAN port
 switchport mode access
 switchport access vlan 10
 no shutdown
 spanning-tree portfast
!
interface Gi1/0/2
 description Trunk to AP
 switchport mode trunk
 switchport trunk allowed vlan 10,20
 no shutdown
!
interface Gi1/0/3
 description Uplink to Core
 switchport mode trunk
 switchport trunk allowed vlan 10,20,30
 no shutdown
!
interface TenGigabitEthernet1/0/3
 description Uplink to Core
 switchport mode trunk
 switchport trunk allowed vlan 10,20,30
 no shutdown
!
interface Gi1/0/4
 description Parking Lot
 switchport mode access
 switchport access vlan 999
 shutdown
!
interface Loopback0
 ip address 10.2.3.1 255.255.255.255
!
snmp-server community asdf RO
snmp-server location asdfg
!
ntp server 0.eu.pool.ntp.org
ntp server 1.eu.pool.ntp.org
!
logging host 192.0.2.100
logging trap informational
!
ip domain-name eu.corp.com
ip name-server 8.8.8.8
ip name-server 8.8.4.4
!
tacacs-server key asdf
!
ip access-list extended Management_access
 permit ip 192.0.2.10
 permit ip 192.0.2.11
 deny ip any
!